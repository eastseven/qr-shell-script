<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/main_content_more_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:orientation="vertical" >

    <Button
        android:id="@+id/main_more_logout"
        style="@style/arrow_button_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_more_logout" />

    <Button
        android:id="@+id/main_more_my_order"
        style="@style/arrow_button_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_more_my_order" />

    <Button
        android:id="@+id/main_more_my_coupon"
        style="@style/arrow_button_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_more_my_coupon" />

    <Button
        android:id="@+id/main_more_my_account"
        style="@style/arrow_button_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_more_my_account" />

    <Button
        android:id="@+id/main_more_faq"
        style="@style/arrow_button_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_more_faq" />

</LinearLayout>